{
    "server": {
        "accessLog": "/var/log/nginx/access.log",
        "port": 80,
        "workers": 10,
        "maxSockets": 100,
        "deadBackendTTL": 300,
        "tcpTimeout": 30,
        "retryOnError": 3,
        "deadBackendOn500": true,
        "httpKeepAlive": false
    },
    "driver": ["redis://127.0.0.1:6379"]
}